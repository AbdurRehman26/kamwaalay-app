{
  "expo": {
    "name": "kamwaalay-app",
    "slug": "kamwaalay-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kamwaalayapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kamwaalay.app"
    },
    "android": {
      "package": "com.kamwaalay.app",
      "versionCode": 3,
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#2563EB",
          "dark": {
            "backgroundColor": "#2563EB"
          },
          "android": {
            "resizeMode": "contain",
            "backgroundColor": "#2563EB"
          },
          "ios": {
            "resizeMode": "cover",
            "backgroundColor": "#2563EB"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "79a7e21d-6f4b-4c96-b3da-9b62ee1f6bd4"
      }
    }
  }
}